<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="469.0" prefWidth="609.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="esprit.petrofactory.interfaces.FXGasPumpController">
   <children>
      <TableView fx:id="tab_gas" layoutX="37.0" layoutY="14.0" prefHeight="190.0" prefWidth="542.0">
        <columns>
          <TableColumn fx:id="id_num" prefWidth="150.0" text="Number" />
          <TableColumn fx:id="id_type" prefWidth="200.0" text="Fuel type" />
            <TableColumn fx:id="id_amount" prefWidth="200.0" text="Amount" />
        </columns>
      </TableView>
      <Button fx:id="btn_delGas" layoutX="483.0" layoutY="235.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="96.0" text="Delete">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="btn_addGas" layoutX="483.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#add" prefHeight="34.0" prefWidth="96.0" text="Add">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TextField fx:id="tf_num" layoutX="14.0" layoutY="302.0" prefHeight="34.0" prefWidth="86.0" />
      <TextField fx:id="tf_type" layoutX="141.0" layoutY="302.0" prefHeight="34.0" prefWidth="134.0" />
      <TextField fx:id="tf_amount" layoutX="318.0" layoutY="303.0" prefHeight="33.0" prefWidth="134.0" />
   </children>
</AnchorPane>
